{
  "name": "boldr-plugin-webpack",
  "version": "0.0.10",
  "description": "Webpack plugin for Boldr.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && babel src --out-dir lib",
    "clean": "rimraf lib",
    "format": "prettier --print-width=100 --single-quote=true --jsx-bracket-same-line=false --trailing-comma=all --write \"src/**/*.js\"",
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "webpack",
    "boldr",
    "build-tool",
    "plugin"
  ],
  "author": "Steven Truesdell <steven@strues.io>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boldr/boldr-plugin-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/boldr/boldr-plugin-webpack/issues"
  },
  "homepage": "https://github.com/boldr/boldr-plugin-webpack#readme",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "lint-staged": {
   "*.js": [
     "prettier --print-width=100 --single-quote=true --jsx-bracket-same-line=false --trailing-comma=all --write \"src/**/*.js\"",
     "git add"
   ]
 },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^4.0.0",
    "eslint-config-boldr": "^0.10.3",
    "husky": "^0.13.4",
    "lint-staged": "^4.0.0",
    "prettier": "^1.4.4",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-import-inspector": "^2.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-styled-components": "^1.1.4",
    "bluebird": "^3.5.0",
    "boldr-engine": "^0.1.1",
    "boldr-root": "^0.0.1",
    "boldr-utils": "^0.4.0-rc.1",
    "cache-loader": "^1.0.3",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^1.1.3",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "circular-dependency-plugin": "^3.0.0",
    "css-loader": "^0.28.4",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "fs-extra": "^3.0.1",
    "happypack": "4.0.0-beta.1",
    "import-inspector": "^2.0.0",
    "lodash-webpack-plugin": "^0.11.4",
    "md5": "^2.2.1",
    "node-fetch": "^1.7.1",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^2.11.0",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "promise": "^7.3.0",
    "react-dev-utils": "^3.0.0",
    "react-hot-loader": "3.0.0-beta.7",
    "sass-loader": "^6.0.6",
    "source-map-support": "^0.4.15",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.18.2",
    "terminate": "^2.1.0",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.4",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.6.0",
    "whatwg-fetch": "^2.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  }
}
